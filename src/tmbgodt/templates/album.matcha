{> with albums as List(Album)

{> import tmbgodt/album.{type Album}
{> import gleam/int

<table class="table-auto">
    <thead>
        <tr>
        <th>Album</th>
        <th>Type</th>
        <th>Year</th>
        </tr>
    </thead>
    {% for album as Album in albums %}
    <tr>
        <td>{{album.name}}</td>
        <td>{{album.album_type}}</td>
        <td>{{int.to_string(album.year)}}</td>
    </tr>
    {% endfor %}
</table>